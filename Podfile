# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

def shared_pods
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  inhibit_all_warnings!

  # Pods for MVC-boilerplate
  pod 'Alamofire', '5.2.2'
  pod 'SwiftLint'
  pod 'FacebookSDK'
  pod 'Firebase/Crashlytics'
  pod 'GoogleSignIn', '5.0.2'
  pod 'SwiftMaskTextfield', '1.1.0'
  pod 'SDWebImage', '4.4.3'
end
 
target 'MVC-boilerplate' do
  # Pods for MVC-boilerplate
  shared_pods
end

target 'MVC-boilerplate Stage' do
  # Pods for MVC-boilerplate
  shared_pods
end

target 'MVC-boilerplate Dev' do
  # Pods for MVC-boilerplate
  shared_pods
end
